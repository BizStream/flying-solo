{
    "manifest_version": 3,
    "name": "Redirect Button with Settings",
    "version": "1.0",
    "description": "A simple button that redirects you with customizable settings.",
    "permissions": ["storage", "activeTab"], 
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "options_page": "options.html",
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }